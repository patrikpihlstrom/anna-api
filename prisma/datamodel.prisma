type User {
  id: ID! @id
  name: String!
  key: String
}

type Job {
  id: ID! @id
  container: String
  worker: String
  driver: String!
  site: String!
  status: String! @default(value: "pending")
  log: String
  history: [Diff!]! @relation(name: "History")
}

type Diff {
	id: ID! @id
	job: Job @relation(name: "History" link: INLINE)
	field: String!
	pre: String!
	post: String!
}
